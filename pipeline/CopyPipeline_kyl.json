{
	"name": "CopyPipeline_kyl",
	"properties": {
		"activities": [
			{
				"name": "Copy_kyl",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 5,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "fd001//azure-data-factory-by-example-main.zip"
					},
					{
						"name": "Destination",
						"value": "sampledate/output/*.*"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"maxConcurrentConnections": 1
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DT_FILENAME",
						"type": "DatasetReference",
						"parameters": {
							"filename": "sample1.csv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "dt_output",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}